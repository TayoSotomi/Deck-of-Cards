@{
}
<form asp-method="post" asp-action="DeckOfCards">
         

    <input type="submit" />
</form>

<h1>These are your Cards</h1>

@{
    foreach(Card c in Model.cards)
    {
        <p>@c.value</p>
        <p>@c.suit</p>

        <img src="@c.image" />

    }
    

}